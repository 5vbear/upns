profile=$1

launcher='com.telecom.ctu.platform.resource.task.TaskNodeLauncher'
PID=`ps -ef | grep $launcher| grep $profile  | grep -v ' grep' | awk '{print $2}'`

echo $PID

if [ ! -e $PID ];
then
        echo "kill myctu resource task node PID is $PID"
        kill $PID
else
        echo "myctu resource task node not run"
fi